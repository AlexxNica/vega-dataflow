{
  "name": "dataflow",
  "version": "0.2.0",
  "description": "Reactive dataflow.",
  "keywords": [
    "dataflow",
    "reactive"
  ],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jeffrey Heer",
    "url": "http://idl.cs.washington.edu"
  },
  "main": "build/dataflow.js",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/uwdata/dataflow.git"
  },
  "scripts": {
    "build": "npm run test && uglifyjs build/dataflow.js -c -m -o build/dataflow.min.js",
    "pretest": "rm -rf build && mkdir build && json2module package.json > build/package.js && bin/rollup",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test"
  },
  "dependencies": {
    "d3-array": "0.7.1",
    "d3-collection": "0.2.0",
    "d3-force": "0.6.3",
    "d3-hierarchy": "0.2.4",
    "d3-scale": "0.7.2",
    "d3-voronoi": "0.3.3"
  },
  "devDependencies": {
    "eslint": "2",
    "json2module": "0.0",
    "rollup": "0.26",
    "rollup-plugin-node-resolve": "1",
    "tape": "4",
    "uglify-js": "2"
  }
}